@article{PathControlIkineAnnLou,
author = {Y F Lou and P Brunn},
title ={A hybrid artificial neural network inverse kinematic solution for accurate robot path control},
journal = {Proceedings of the Institution of Mechanical Engineers, Part I: Journal of Systems and Control Engineering},
volume = {213},
